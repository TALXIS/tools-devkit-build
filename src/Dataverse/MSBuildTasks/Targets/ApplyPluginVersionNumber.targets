<Project>
	<Target Name="ApplyPluginVersionNumber">
		<Message Text="Applying version to $(Version) in plugin..." Importance="high" />
		<PropertyGroup>
			<AssemblyVersion>$(Version)</AssemblyVersion>
			<FileVersion>$(Version)</FileVersion>
			<Version>$(Version)</Version>
			<PackageVersion>$(Version)</PackageVersion>
		</PropertyGroup>
	</Target>
</Project>